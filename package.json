{
  "name": "compo-highlight.js",
  "version": "0.8.12",
  "homepage": "https://github.com/atmajs/compo-highlight.js",
  "description": "Highlight.js wrapper for MaskJS",
  "main": "lib/Highlight.js",
  "devDependencies": {
    "atma": ">0.9.26",
    "atma-loader-babel": ">0.0.0",
    "maskjs": "^0.54.16"
  },  
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "es6",
          "test"
        ]
      }
    }
  },
  "dependencies": {
    "maskjs": "latest"
  }
}
